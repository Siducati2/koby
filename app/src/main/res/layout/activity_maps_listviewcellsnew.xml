<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    <RelativeLayout
        android:id="@+id/rl_cell"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="@color/color_trans">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@color/color_trans">
            <RelativeLayout
                android:id="@+id/relativeLayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="false"
                android:layout_alignParentTop="true"
                android:layout_alignParentBottom="true">
                <ImageView
                    android:id="@+id/img_thumb"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:background="@color/iron"
                    android:contentDescription="icon" />
                <com.innohawk.dan.LoaderImageView
                    android:id="@+id/media_images_image_view"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:contentDescription="spinner image"
                    android:background="@color/color_trans"/>

                <TextView
                    android:id="@+id/text_title"
                    android:layout_width="60dp"
                    android:layout_height="15dp"
                    android:layout_alignBottom="@+id/txt_age"
                    android:layout_alignTop="@+id/img_thumb"
                    android:layout_toEndOf="@+id/img_thumb"
                    android:layout_toLeftOf="@+id/txt_age"
                    android:layout_toRightOf="@+id/img_thumb"
                    android:layout_toStartOf="@+id/txt_age"
                    android:contentDescription="icon"
                    android:gravity="center_horizontal|left"
                    android:text=" - "
                    android:maxLines="1"
                    android:textColor="#000"
                    android:textStyle="bold"/>

                <TextView
                    android:id="@+id/txt_age"
                    android:layout_width="100dp"
                    android:layout_height="20dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignTop="@+id/img_thumb"
                    android:layout_marginRight="5dp"
                    android:gravity="center_vertical|right"
                    android:text=" - "
                    android:textColor="@color/monsoon"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/txt_distance"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/text_title"
                    android:layout_toEndOf="@+id/img_thumb"
                    android:layout_toRightOf="@+id/img_thumb"
                    android:gravity="left"
                    android:text=" - "
                    android:textColor="@color/monsoon"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/txt_sex"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/txt_distance"
                    android:layout_toEndOf="@+id/img_thumb"
                    android:layout_toRightOf="@+id/img_thumb"
                    android:gravity="left"
                    android:text=" - "
                    android:textColor="@color/aluminum"
                    android:textSize="10sp" />
            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>

</RelativeLayout>